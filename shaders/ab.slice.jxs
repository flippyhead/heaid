<jittershader name="slice">	<param name="texo" type = "int" default="0" />	<param name="texa" type = "int" default="1" />		<language name="glsl" version="1.0">		<bind param="slice" program="fp" />		<bind param="texo" program="fp" />		<bind param="texa" program="fp" />		<program name="vp" type="vertex" source="sh.passthrudim.vp.glsl" />		<program name="fp" type="fragment"> <![CDATA[varying vec2 texcoord0;varying vec2 texcoord1;uniform float slice;uniform sampler3D texo;uniform sampler2DRect texa;const vec4 coeff = vec4(0.299, .587, 0.114, 0.);void main( void ){	float v1 = dot(texture2DRect(texa,texcoord0*vec2(512.)),coeff);	//assumes 512  x 512 slice map.  Pretty arbitrary...	vec4 v0 = texture3D(texo, vec3(texcoord0.xy,v1));	gl_FragColor = v0;}]]>		</program>	</language></jittershader>